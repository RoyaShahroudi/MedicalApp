<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="550.0" prefWidth="900.0" style="-fx-background-color: #132330;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="medicalApp.Patient.SearchDr">
   <children>
      <Pane layoutX="710.0" prefHeight="550.0" prefWidth="200.0" style="-fx-background-color: #243441;">
         <children>
            <VBox fx:id="vboxbtnoage" layoutY="175.0" prefHeight="376.0" prefWidth="200.0" style="-fx-background-color: #354552;">
               <children>
                  <Button fx:id="visitBtn" mnemonicParsing="false" onAction="#visitBtn" prefHeight="50.0" prefWidth="200.0" style="-fx-background-radius: 0; -fx-background-color: #354552;" text="درخواست ویزیت" textFill="WHITE">
                     <font>
                        <Font name="Comic Sans MS" size="20.0" />
                     </font>
                  </Button>
                  <Button fx:id="editProBtn" mnemonicParsing="false" onAction="#editProBtn" prefHeight="50.0" prefWidth="200.0" style="-fx-background-radius: 0; -fx-background-color: #354552;" text="ویرایش پروفایل" textFill="WHITE">
                     <font>
                        <Font name="Comic Sans MS" size="20.0" />
                     </font>
                  </Button>
                  <Button fx:id="chatBtn" mnemonicParsing="false" onAction="#chatBtn" prefHeight="50.0" prefWidth="200.0" style="-fx-background-radius: 0; -fx-background-color: #354552;" text="چت ها" textFill="WHITE">
                     <font>
                        <Font name="Comic Sans MS" size="20.0" />
                     </font>
                  </Button>
                  <Button fx:id="medicationReminder" mnemonicParsing="false" onAction="#medicationReminder" prefHeight="50.0" prefWidth="200.0" style="-fx-background-radius: 0; -fx-background-color: #354552;" text="زمانبندی دارویی" textFill="WHITE">
                     <font>
                        <Font name="Comic Sans MS" size="20.0" />
                     </font>
                  </Button>
                  <Button fx:id="searchBtn" mnemonicParsing="false" prefHeight="50.0" prefWidth="200.0" style="-fx-background-radius: 0; -fx-background-color: #132330;" text="جستجو پزشک" textFill="WHITE">
                     <font>
                        <Font name="Comic Sans MS" size="20.0" />
                     </font>
                  </Button>
                  <Button fx:id="exitBtn" mnemonicParsing="false" onAction="#exitBtn" prefHeight="50.0" prefWidth="200.0" style="-fx-background-radius: 0; -fx-background-color: #354552;" text="خروج" textFill="#a4a4a4">
                     <font>
                        <Font name="Comic Sans MS" size="20.0" />
                     </font>
                  </Button>
               </children>
            </VBox>
            <ImageView fx:id="paProImage" fitHeight="100.0" fitWidth="100.0" layoutX="77.0" layoutY="23.0" pickOnBounds="true" preserveRatio="true" />
            <Label fx:id="nameLabel" alignment="CENTER" layoutX="12.0" layoutY="132.0" prefHeight="30.0" prefWidth="177.0" textFill="WHITE">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <Button fx:id="infoBtn" layoutX="314.0" layoutY="345.0" mnemonicParsing="false" onAction="#infoBtn" prefHeight="25.0" prefWidth="117.0" text="اطلاعات پزشک" />
      <Button fx:id="chatToDrBtn" layoutX="314.0" layoutY="415.0" mnemonicParsing="false" onAction="#chatToDrBtn" prefHeight="25.0" prefWidth="117.0" text="چت با پزشک" />
      <Label layoutX="233.0" layoutY="43.0" prefHeight="27.0" prefWidth="285.0" text="نام و نام خانوادگی پزشک را وارد کنید" textFill="WHITE">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="searchDrNameField" alignment="CENTER_RIGHT" layoutX="269.0" layoutY="125.0" prefHeight="25.0" prefWidth="212.0" promptText="نام" />
      <TextField fx:id="searchDrLastNField" alignment="CENTER_RIGHT" layoutX="266.0" layoutY="208.0" prefHeight="25.0" prefWidth="212.0" promptText="نام خانوادگی" />
      <Label fx:id="errorLabel" alignment="CENTER" layoutX="245.0" layoutY="275.0" prefHeight="30.0" prefWidth="259.0" textFill="#c32929">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
